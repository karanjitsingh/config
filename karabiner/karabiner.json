{
    "global": { "show_in_menu_bar": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Press both left and right shift together to toggle Caps Lock",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_shift",
                                    "modifiers": {
                                        "mandatory": ["right_shift"],
                                        "optional": ["caps_lock"]
                                    }
                                },
                                "to": [{ "key_code": "caps_lock" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_shift",
                                    "modifiers": {
                                        "mandatory": ["left_shift"],
                                        "optional": ["caps_lock"]
                                    }
                                },
                                "to": [{ "key_code": "caps_lock" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Remap Right Option (Alt) + L to Control + L",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "l",
                                    "modifiers": { "mandatory": ["right_option"] }
                                },
                                "to": [
                                    {
                                        "key_code": "l",
                                        "modifiers": ["control"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Caps Lock + Left Control acts as Left Control",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "right_control" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Open Apps by left option + num",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "1",
                                    "modifiers": { "mandatory": ["left_option"] }
                                },
                                "to": [{ "software_function": { "open_application": { "file_path": "/Applications/Arc.app" } } }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "2",
                                    "modifiers": { "mandatory": ["left_option"] }
                                },
                                "to": [{ "software_function": { "open_application": { "file_path": "/Applications/Cursor.app" } } }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "4",
                                    "modifiers": { "mandatory": ["left_option"] }
                                },
                                "to": [{ "software_function": { "open_application": { "file_path": "/Applications/WhatsApp.app" } } }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "3",
                                    "modifiers": { "mandatory": ["left_option"] }
                                },
                                "to": [{ "software_function": { "open_application": { "file_path": "/Applications/iTerm.app" } } }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "5",
                                    "modifiers": { "mandatory": ["left_option"] }
                                },
                                "to": [{ "software_function": { "open_application": { "file_path": "/Applications/Spotify.app" } } }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "q",
                                    "modifiers": { "mandatory": ["left_option"] }
                                },
                                "to": [{ "software_function": { "open_application": { "file_path": "/Applications/Signal.app" } } }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "w",
                                    "modifiers": { "mandatory": ["left_option"] }
                                },
                                "to": [{ "software_function": { "open_application": { "file_path": "/Applications/Obsidian.app" } } }],
                                "type": "basic"
                            }
                            
                        ]
                    },
                    {
                        "description": "Escape as Layer for Vim Navigation with hjkl and Escape when tapped",
                        "manipulators": [
                            {
                                "from": { "key_code": "escape" },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "nav_layer",
                                            "value": 1
                                        }
                                    },
                                    { "key_code": "escape" }
                                ],
                                "to_after_key_up": [
                                    {
                                        "set_variable": {
                                            "name": "nav_layer",
                                            "value": 0
                                        }
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "name": "nav_layer",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": { "key_code": "h" },
                                "to": [{ "key_code": "left_arrow" }],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "name": "nav_layer",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": { "key_code": "j" },
                                "to": [{ "key_code": "down_arrow" }],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "name": "nav_layer",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": { "key_code": "k" },
                                "to": [{ "key_code": "up_arrow" }],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "name": "nav_layer",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": { "key_code": "l" },
                                "to": [{ "key_code": "right_arrow" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Map Cmd+P to Cmd+J in Quip.app only",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "com.quip.Desktop"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "p",
                                    "modifiers": {
                                        "mandatory": ["left_command"],
                                        "optional": ["right_command"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "j",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change control+hjkl to arrow keys",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "h",
                                    "modifiers": {
                                        "mandatory": ["control"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "left_arrow" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "j",
                                    "modifiers": {
                                        "mandatory": ["control"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "down_arrow" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "k",
                                    "modifiers": {
                                        "mandatory": ["control"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "up_arrow" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "l",
                                    "modifiers": {
                                        "mandatory": ["left_control"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "right_arrow" }],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "name": "Default profile",
            "selected": true,
            "simple_modifications": [
                {
                    "from": { "key_code": "caps_lock" },
                    "to": [{ "key_code": "escape" }]
                }
            ],
            "virtual_hid_keyboard": { "keyboard_type_v2": "ansi" }
        }
    ]
}